{
  "name": "@borilla/object-pool",
  "version": "0.1.0",
  "description": "Reusable pool of objects to reduce garbage collection",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015-loose --out-dir lib/ src/",
    "minify": "uglifyjs --compress --mangle --screw-ie8 --output lib/index.min.js lib/index.js",
    "lint": "eslint src/ test/",
    "unit-test": "mocha test/",
    "test": "npm run compile && npm run lint && npm run unit-test",
    "prepublish": "npm test"
  },
  "keywords": [
    "object",
    "pool",
    "garbage",
    "collection"
  ],
  "author": "borilla",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015-loose": "^7.0.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "uglify-js": "^2.6.2"
  }
}
